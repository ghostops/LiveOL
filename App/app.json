{
  "expo": {
    "name": "LiveOL",
    "slug": "LiveOL",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.1.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e86a1e"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 5000,
      "checkAutomatically": "ON_LOAD"
    },
    "assetBundlePatterns": [
      "./assets/**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "se.liveol.rn"
    },
    "android": {
      "package": "se.liveol.rn",
      "versionCode": 7,
      "permissions": []
    }
  }
}
