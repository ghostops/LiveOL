{
	"name": "@liveol/app",
	"version": "2.4.1",
	"scripts": {
		"start": "react-native start",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"test": "node ./node_modules/jest/bin/jest.js --watchAll",
		"lint": "yarn eslint ./src",
		"release": "expo publish --target bare && git push",
		"lang:pull": "phrase pull",
		"lang:push": "phrase push",
		"schema": "yarn schema:download && yarn schema:tots && yarn schema:cleanup",
		"schema:download": "yarn run apollo schema:download --endpoint=http://localhost:4000 graphql-schema.json",
		"schema:tots": "yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/lib/graphql/types/graphql-global-types.ts types",
		"schema:cleanup": "yarn rimraf graphql-schema.json types"
	},
	"jest": {
		"preset": "jest-expo"
	},
	"dependencies": {
		"@apollo/react-hooks": "^3.1.3",
		"@expo/samples": "2.1.1",
		"@expo/vector-icons": "^12.0.0",
		"@react-native-community/masked-view": "0.1.10",
		"@react-navigation/native": "^5.9.4",
		"@react-navigation/stack": "^5.14.4",
		"apollo-boost": "^0.4.7",
		"expo": "^41.0.0",
		"expo-constants": "~10.1.3",
		"expo-font": "~9.1.0",
		"expo-localization": "~10.1.0",
		"expo-permissions": "~12.0.1",
		"expo-splash-screen": "~0.10.2",
		"expo-status-bar": "~1.0.4",
		"expo-updates": "~0.5.4",
		"graphql": "^14.6.0",
		"lodash": "^4.17.21",
		"moment": "^2.25.3",
		"moment-duration-format": "^2.3.2",
		"ms": "^2.1.2",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-native": "0.63.4",
		"react-native-easy-grid": "^0.2.2",
		"react-native-gesture-handler": "~1.10.2",
		"react-native-hyperlink": "^0.0.19",
		"react-native-reanimated": "~2.1.0",
		"react-native-restart": "^0.0.17",
		"react-native-safe-area-context": "3.2.0",
		"react-native-screens": "~3.0.0",
		"react-native-size-matters": "^0.3.0",
		"react-native-svg": "12.1.0",
		"react-native-toast-message": "^1.4.7",
		"react-native-unimodules": "~0.13.3",
		"react-native-web": "~0.13.12",
		"react-redux": "^7.1.3",
		"redux": "^4.0.4",
		"redux-persist": "^6.0.0",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/core": "~7.9.0",
		"@babel/runtime": "^7.8.4",
		"@types/graphql": "^14.5.0",
		"@types/lodash": "^4.14.120",
		"@types/react": "~16.9.35",
		"@types/react-native": "~0.63.2",
		"@types/react-navigation": "^3.0.1",
		"@types/react-redux": "^7.1.5",
		"@types/redux": "^3.6.0",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"apollo": "^2.25.0",
		"babel-jest": "~25.2.6",
		"babel-plugin-module-resolver": "^3.1.2",
		"babel-preset-expo": "8.3.0",
		"eslint": "^7.10.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-import-resolver-babel-module": "^4.0.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.3",
		"eslint-plugin-react-hooks": "^4.1.2",
		"express": "^4.16.4",
		"jest": "~25.2.6",
		"jest-expo": "^41.0.0",
		"nodemon": "^1.18.10",
		"prettier": "^2.1.2",
		"react-test-renderer": "~16.11.0",
		"rimraf": "^3.0.2",
		"sftp-sync-deploy": "^0.7.1",
		"typescript": "~4.0.0"
	},
	"private": true
}
