{
  "name": "@liveol/app",
  "version": "2.2.0",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "yarn tslint --project ./",
    "release": "yarn lang:pull && npm version patch && expo publish && git push",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "testserver": "nodemon testserver/",
    "lang:pull": "phrase pull",
    "lang:push": "phrase push",
    "schema": "yarn schema:download && yarn schema:tots && yarn schema:cleanup",
    "schema:download": "yarn run apollo schema:download --endpoint=http://localhost:4000 graphql-schema.json",
    "schema:tots": "yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/lib/graphql/types/graphql-global-types.ts types",
    "schema:cleanup": "yarn rimraf graphql-schema.json types"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "apollo-boost": "^0.4.7",
    "expo": "^38.0.0",
    "expo-constants": "~9.1.1",
    "expo-font": "~8.2.1",
    "expo-localization": "~8.2.1",
    "expo-permissions": "~9.0.1",
    "expo-splash-screen": "^0.5.0",
    "expo-status-bar": "^1.0.0",
    "expo-updates": "~0.2.8",
    "graphql": "^14.6.0",
    "lodash": "^4.17.19",
    "moment": "^2.25.3",
    "moment-duration-format": "^2.3.2",
    "ms": "^2.1.2",
    "native-base": "^2.13.4",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "~0.62.2",
    "react-native-circular-progress": "^1.3.6",
    "react-native-easy-grid": "^0.2.2",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-restart": "^0.0.17",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-size-matters": "^0.3.0",
    "react-native-svg": "12.1.0",
    "react-native-unimodules": "~0.10.1",
    "react-native-web": "~0.11.7",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.120",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@types/react-navigation": "^3.0.1",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "apollo": "^2.25.0",
    "babel-jest": "~25.2.6",
    "babel-plugin-module-resolver": "^3.1.2",
    "babel-preset-expo": "^8.2.3",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "express": "^4.16.4",
    "jest": "~25.2.6",
    "jest-expo": "^38.0.0",
    "nodemon": "^1.18.10",
    "react-test-renderer": "~16.11.0",
    "rimraf": "^3.0.2",
    "sftp-sync-deploy": "^0.7.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "~3.9.5"
  },
  "private": true
}
