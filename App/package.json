{
  "name": "@liveol/app",
  "version": "1.5.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "yarn tslint --project ./",
    "release": "phraseapp pull && npm version patch && expo publish && git push",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "testserver": "nodemon testserver/",
    "lang": "phraseapp pull",
    "schema": "yarn schema:download && yarn schema:tots",
    "schema:download": "yarn run apollo schema:download --endpoint=http://localhost:4000 graphql-schema.json",
    "schema:tots": "yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/types/graphql-global-types.ts types"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.5",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "apollo-boost": "^0.4.7",
    "expo": "^36.0.0",
    "expo-blur": "~8.0.0",
    "expo-font": "~8.0.0",
    "expo-localization": "~8.0.0",
    "graphql": "^14.6.0",
    "lodash": "^4.17.13",
    "ms": "^2.1.2",
    "native-base": "^2.13.4",
    "react": "16.9.0",
    "react-apollo": "^3.1.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-circular-progress": "^1.3.6",
    "react-native-easy-grid": "^0.2.2",
    "react-native-flags-kit": "^1.0.1",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-svg": "9.13.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.120",
    "@types/react": "^16.9.0",
    "@types/react-native": "^0.60.15",
    "@types/react-navigation": "^3.0.1",
    "@types/react-redux": "^7.1.5",
    "@types/redux": "^3.6.0",
    "apollo": "^2.25.0",
    "babel-plugin-module-resolver": "^3.1.2",
    "babel-preset-expo": "~8.0.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "express": "^4.16.4",
    "jest-expo": "^36.0.0",
    "nodemon": "^1.18.10",
    "sftp-sync-deploy": "^0.7.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "~3.6.3"
  },
  "private": true
}
