require 'dotenv'
Dotenv.load('../.env')

puts '----------'
puts "Email: " + ENV["IOS_EMAIL"]
puts "ITC Team ID: " + ENV["IOS_ITC_TEAM_ID"]
puts "Team ID: " + ENV["IOS_TEAM_ID"]
puts '----------'

app_identifier("se.liveol.rn") # The bundle identifier of your app
apple_id(ENV['IOS_EMAIL']) # Your Apple email address

itc_team_id(ENV['IOS_ITC_TEAM_ID']) # App Store Connect Team ID
team_id(ENV['IOS_TEAM_ID']) # Developer Portal Team ID

